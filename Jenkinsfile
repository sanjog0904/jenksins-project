pipeline{
  agent any
  stages{
    stage ("build"){
      step{
        script{
        echo "building the app..."
        }
      }
    }
     stage ("test"){
      step{
        script{
        echo "testing the app..."
        }
      }
    }    
     stage ("deploy"){
      step{
        script{
        echo "deploying the app..."
        }
      }
    }
  }
}
